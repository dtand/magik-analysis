{
    "name": "Trading Session Positive A-B Close Prediction",
    "description": "Analyze the probability of Session A closing positive implies Session B closing positive",
    "dataSpecifications": [
        {
            "symbol": "$1",
            "source": "LocalFiles",
            "marketType": "FUTURE",
            "timeSeries": "1h",
            "indicators": [
                {
                    "name": "sbotc",
                    "identifier": "SBOTC",
                    "config": { 
                        "pre_session": [0,5],
                        "session_a": [6,11],
                        "session_b": [12,20]
                    }
                }
            ]
        }
    ],
    "analysisSpecification": {
        "assessOn": {
            "var1": {
                "symbol": "$1",
                "var": "timestamp.hour",
                "lookahead": 0
            },
            "op": "eq",
            "var2": {
                "symbol": null,
                "var": 20,
                "lookahead": 0
            }
        },
        "criteria": {
            "var1": {
                "symbol": "$1",
                "var": "SBOTC.aotc",
                "lookahead": 0
            },
            "op": "gt",
            "var2": {
                "symbol": null,
                "var": 0.0,
                "lookahead": 0
            }
        },
        "predict": {
            "var1": {
                "symbol": "$1",
                "var": "SBOTC.botc",
                "lookahead": 0
            },
            "op": "gt",
            "var2": {
                "symbol": null,
                "var": 0.0,
                "lookahead": 0
            }
        }
    },
    "outputSpecification": {
        "format": "csv",
        "variables": [
            {
                "symbol": "$1",
                "var": "timestamp",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "SBOTC.aotc",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "SBOTC.botc",
                "lookahead": 0
            }
        ]
    }
}