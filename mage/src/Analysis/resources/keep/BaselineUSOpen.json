{
    "name": "Probability that US opens green",
    "description": "Probability that US opens green on any given day",
    "dataSpecifications": [
        {
            "symbol": "$1",
            "source": "LocalFiles",
            "marketType": "FUTURE",
            "timeSeries": "1h",
            "indicators": []
        }
    ],
    "analysisSpecification": {
        "assessOn": {
            "var1": {
                "symbol": "$1",
                "var": "timestamp.hour",
                "lookahead": 0
            },
            "op": "eq",
            "var2": {
                "symbol": null,
                "var": 20,
                "lookahead": 0
            }
        },
        "predict": {
            "var1": {
                "symbol": "$1",
                "var": "close",
            "lookahead": 0
            },
            "op": "gt",
            "var2": {
                "symbol": "$1",
                "var": "open",
                "lookahead": 0
            }
        }
    },
    "outputSpecification": {
        "format": "csv",
        "variables": [
            {
                "symbol": "$1",
                "var": "timestamp",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "open",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "close",
                "lookahead": 0
            }
        ]
    }
}