{
    "name": "Trading Session Positive A RSI Threshold Implies Session B High",
    "description": "Analyze the probability of Session A having >= x RSI predicts a positive close in session b",
    "dataSpecifications": [
        {
            "symbol": "$1",
            "source": "LocalFiles",
            "marketType": "FUTURE",
            "timeSeries": "1h",
            "indicators": [
                {
                    "name": "sbotc",
                    "identifier": "SBOTC",
                    "config": { 
                        "pre_session": [0,5],
                        "session_a": [6,11],
                        "session_b": [12,20]
                    }
                },
                {
                    "name": "rsi",
                    "identifier": "RSI",
                    "config": {}
                }
            ]
        }
    ],
    "analysisSpecification": {
        "assessOn": {
            "var1": {
                "symbol": "$1",
                "var": "timestamp.hour",
                "lookahead": 0
            },
            "op": "eq",
            "var2": {
                "symbol": null,
                "var": 20,
                "lookahead": 0
            }
        },
        "criteria": {
            "var1": {
                "symbol": "$1",
                "var": "RSI",
                "lookahead": -8
            },
            "op": "lteq",
            "var2": {
                "symbol": null,
                "var": 30,
                "lookahead": 0
            }
        },
        "predict": {
            "var1": {
                "symbol": "$1",
                "var": "SBOTC.botc",
                "lookahead": 0
            },
            "op": "lt",
            "var2": {
                "symbol": null,
                "var": 0.0,
                "lookahead": 0
            }
        }
    },
    "outputSpecification": {
        "format": "csv",
        "variables": [
            {
                "symbol": "$1",
                "var": "timestamp",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "RSI",
                "lookahead": -8
            },
            {
                "symbol": "$1",
                "var": "SBOTC.botc",
                "lookahead": 0
            }
        ]
    }
}