{
    "name": "Trading Session Low Prediction",
    "description": "Analyze the probability of a subset trading session in some day making a low, to predict the following session low",
    "dataSpecifications": [
        {
            "symbol": "$1",
            "source": "LocalFiles",
            "marketType": "FUTURE",
            "timeSeries": "1h",
            "indicators": [
                {
                    "name": "sbl",
                    "identifier": "SBL",
                    "config": { 
                        "pre_session": [0,5],
                        "session_a": [6,11],
                        "session_b": [12,20]
                    }
                },
                {
                    "name": "rsi",
                    "identifier": "RSI",
                    "config": { 
                        "timeperiod": 12
                    }
                }
            ]
        }
    ],
    "analysisSpecification": {
        "assessOn": {
            "var1": {
                "symbol": "$1",
                "var": "timestamp.hour",
                "lookahead": 0
            },
            "op": "eq",
            "var2": {
                "symbol": null,
                "var": 20,
                "lookahead": 0
            }
        },
        "criteria": {
            "var1": {
                "symbol": "$1",
                "var": "SBL.asl",
                "lookahead": 0
            },
            "op": "lt",
            "var2": {
                "symbol": "$1",
                "var": "SBL.psl",
                "lookahead": 0
            },
            "and": [{
                "var1": {
                    "symbol": "$1",
                    "var": "RSI",
                    "lookahead": -9
                },
                "op": "lt",
                "var2": {
                    "symbol": null,
                    "var": 20,
                    "lookahead": 0
                } 
            }]   
        },
        "predict": {
            "var1": {
                "symbol": "$1",
                "var": "SBL.bsl",
                "lookahead": 0
            },
            "op": "lt",
            "var2": {
                "symbol": "$1",
                "var": "SBL.asl",
                "lookahead": 0
            }
        }
    },
    "outputSpecification": {
        "format": "csv",
        "variables": [
            {
                "symbol": "$1",
                "var": "timestamp",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "SBL.psl",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "SBL.asl",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "SBL.bsl",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "RSI",
                "lookahead": -9
            }
        ]
    }
}