{
    "name": "RSI Low Analysis",
    "description": "Output RSI values against US session lows",
    "dataSpecifications": [
        {
            "symbol": "$1",
            "source": "LocalFiles",
            "marketType": "FUTURE",
            "timeSeries": "1h",
            "indicators": [
                {
                    "name": "sbl",
                    "identifier": "SBL",
                    "config": { 
                        "pre_session": [0,5],
                        "session_a": [6,12],
                        "session_b": [13,20]
                    }
                },
                {
                    "name": "sbh",
                    "identifier": "SBH",
                    "config": { 
                        "var": "high",
                        "pre_session": [0,5],
                        "session_a": [6,12],
                        "session_b": [13,20]
                    }
                },
                {
                    "name": "rsi",
                    "identifier": "RSI",
                    "config": { 
                        "timeperiod": 12
                    }
                }
            ]
        }
    ],
    "analysisSpecification": {
        "assessOn": {
            "var1": {
                "symbol": "$1",
                "var": "timestamp.hour",
                "lookahead": 0
            },
            "op": "eq",
            "var2": {
                "symbol": null,
                "var": 20,
                "lookahead": 0
            }
        }
    },
    "outputSpecification": {
        "format": "csv",
        "variables": [
            {
                "symbol": "$1",
                "var": "timestamp",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "SBH.bsh",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "SBL.bsl",
                "lookahead": 0
            },
            {
                "symbol": "$1",
                "var": "RSI",
                "lookahead": 0
            }
        ]
    }
}